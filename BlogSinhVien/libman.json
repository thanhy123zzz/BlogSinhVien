{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "Chart.js@4.1.1",
      "destination": "wwwroot/js/",
      "files": [
        "chart.min.js"
      ]
    },
    {
      "library": "font-awesome@6.2.1",
      "destination": "wwwroot/font-awesome/",
      "files": [
        "css/all.min.css"
      ]
    }
  ]
}